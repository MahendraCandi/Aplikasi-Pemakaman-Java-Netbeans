	<queryString>
		<![CDATA[SELECT
     dataarsip.`NoArsip` AS dataarsip_NoArsip,
     dataarsip.`TanggalBuatIzin` AS dataarsip_TanggalBuatIzin,
     dataarsip.`TanggalHabisIzin` AS dataarsip_TanggalHabisIzin,
     dataarsip.`NoJenazah` AS dataarsip_NoJenazah,
     dataarsip.`NoWaris` AS dataarsip_NoWaris,
     dataarsip.`KodeBlok` AS dataarsip_KodeBlok,
     dataarsip.`Keterangan` AS dataarsip_Keterangan,
     dataarsip.`KodeUser` AS dataarsip_KodeUser,
     datatransaksi.`BiayaSewa` AS datatransaksi_BiayaSewa,
     datatransaksi.`BiayaRumput` AS datatransaksi_BiayaRumput,
     datatransaksi.`BiayaNisan` AS datatransaksi_BiayaNisan,
     datatransaksi.`BiayaKeramik` AS datatransaksi_BiayaKeramik,
     datatransaksi.`TotalBiaya` AS datatransaksi_TotalBiaya,
     datatransaksi.`NoArsip` AS datatransaksi_NoArsip
FROM
     `dataarsip` dataarsip

INNER JOIN
	`datatransaksi` datatransaksi
ON
	dataarsip.`NoArsip`=datatransaksi.`NoArsip`
WHERE
	dataarsip.`TanggalBuatIzin`
BETWEEN
	$P{tgl1}
AND
	$P{tgl2}]]>
	</queryString>
